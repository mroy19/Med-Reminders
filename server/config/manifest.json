{
  "connections": [
    {
      "host": "localhost",
      "port": "3003",
      "labels": ["api"]
    }
  ],
  "registrations": [
    {
      "plugin": "hapi-auth-jwt2"
    },
    {
      "plugin": {
        "register": "dogwater",
        "options": {
          "adapters": {
            "memory": "sails-memory",
            "mysql": "sails-mysql"
          },
          "connections": {
            "myql": {
              "adapter": "mysql"
            },
            "memory": {
              "adapter": "memory"
            }
          },
          "models": "server/lib/modules/user/user.model.js"
        }
      }
    },
    {
      "plugin": "./auth"
    },
    {
      "plugin": "./user"
    },
    {
      "plugin": {
        "register": "good",
        "options": {
          "reporters": [
            {
              "reporter":"good-console",
              "events": {
                "log": "*",
                "response": "*"
              }
            }
          ]
        }
      }
    }
  ]
}
